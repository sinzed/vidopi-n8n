{
  "name": "n8n-nodes-vidopi",
  "version": "1.2.7",
  "description": "n8n custom nodes for Vidopi video processing API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "logo.png"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vidopi/n8n-nodes-vidopi.git"
  },
  "homepage": "https://github.com/Vidopi/n8n-nodes-vidopi#readme",
  "bugs": {
    "url": "https://github.com/Vidopi/n8n-nodes-vidopi/issues"
  },
  "scripts": {
    "build": "tsc && node scripts/postbuild.js",
    "dev": "n8n",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\""
  },
  "keywords": [
    "n8n",
    "n8n-community-node",
    "n8n-community-node-package",
    "vidopi",
    "video",
    "video-processing",
    "api"
  ],
  "author": {
    "name": "Saeed Zahedi",
    "email": "development@vidopi.com"
  },
  "license": "MIT",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "./dist/nodes/Vidopi/UploadVideo/UploadVideo.node.js",
      "./dist/nodes/Vidopi/CutVideo/CutVideo.node.js",
      "./dist/nodes/Vidopi/MergeVideos/MergeVideos.node.js",
      "./dist/nodes/Vidopi/ResizeVideo/ResizeVideo.node.js",
      "./dist/nodes/Vidopi/TaskStatus/TaskStatus.node.js",
      "./dist/nodes/Vidopi/Wait/Wait.node.js"
    ],
    "credentials": [
      "./dist/credentials/VidopiApi.credentials.js"
    ]
  },
  "devDependencies": {
    "n8n-workflow": "^1.64.0",
    "@eslint/js": "^9.39.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.14.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.46.3"
  }
}
