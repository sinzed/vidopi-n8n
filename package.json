{
  "name": "n8n-nodes-vidopi",
  "version": "1.0.7",
  "description": "n8n custom nodes for Vidopi video processing API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "logo.png"
  ],
  "scripts": {
    "build": "tsc && node scripts/postbuild.js",
    "dev": "n8n",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\""

  },
  "keywords": [
    "n8n",
    "n8n-community-node",
    "n8n-community-node-packag",
    "n8n-community-node-package",
    "vidopi",
    "video",
    "video-processing",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "./dist/nodes/Vidopi/UploadVideo/UploadVideo.js",
      "./dist/nodes/Vidopi/CutVideo/CutVideo.js",
      "./dist/nodes/Vidopi/MergeVideos/MergeVideos.js",
      "./dist/nodes/Vidopi/ResizeVideo/ResizeVideo.js",
      "./dist/nodes/Vidopi/TaskStatus/TaskStatus.js"
    ],
    "credentials": [
      "./dist/credentials/VidopiApi.credentials.js"
    ]
  },
  "dependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^9.14.0",
    "@eslint/js": "^9.39.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.46.3"
  }
}
